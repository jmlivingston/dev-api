{
  "name": "dev-api",
  "version": "1.0.1",
  "scripts": {
    "start": "npm run build -- port:3001 && npm run serve -- port:3001",
    "start-with-tunnel": "npm run && npm run create-tunnel",
    "build": "node build",
    "serve": "node serve",
    "create-tunnel": "ngrok tcp --remote-addr 1.tcp.ngrok.io:29182 3001"
  },
  "dependencies": {
    "express-jwt": "^5.1.0",
    "json-server": "^0.8.22",
    "jsonwebtoken": "^7.1.9",
    "nodemon": "^1.11.0",
    "opn": "^4.0.2"
  }
}
